{
  "name": "GetResponse",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "A chrome extension for get http/s's response",
  "content_scripts": [
    {
      "matches": [
        "*://*.yourweb.com/*"
      ],
      "js": [
        "./content.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "web_accessible_resources": ["injected.js"]
}